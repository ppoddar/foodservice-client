<table class='w3-striped'>
    <thead>
    <tr>
        <th>Date</th>
        <th>Title</th>
        <th>Amount</th>
        <th>Who</th>
        <th>Comment</th>
    </tr>
    </thead>
    <tbody>
        {{#items}}
        <tr>
            <td>{{date}}</td>
            <td>{{title}}</td>
            <td>{{amount}}</td>
            <td>{{user}}</td>
            <td>{{comment}}</td>
        </tr>
        {{/items}}
    </tbody>
</table>